

<div class="container px-5 my-1 bg-dark text-white">
{{ form_start(form) }}
<fieldset>

     <div class="form-group">
            <input type="hidden" class="form-control"  readonly value= "{{matricule}}">
     </div>
     <div class="form-group">
            {{form_row(form.prenom)}}
     </div>
     <div class="form-group">
            {{form_row(form.nom)}}
     </div>
     <div class="form-group">
            {{form_row(form.datenaisse,{'attr': {'placeholder':'Date naissance', 'attr':'datepicker'}})}}
     <div class="form-group">
            {{form_row(form.telephone)}}
     </div>
     <div class="form-group">
            {{form_row(form.service)}}
     </div>
     <div class="form-group">
            {{form_row(form.specialite)}}
     </div>
     <div class="form-group">

    <button class="btn btn-success" >{{ button_label|default('Enregister') }}</button>
    <a href="{{ path('medecin_index') }}" class="btn btn-success">Voir liste</a> 

   

     </div>
</fieldset>
</br>
</div>
</div>
</div>
{{ form_end(form) }}
